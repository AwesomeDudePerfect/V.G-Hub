local OldNameCall = nil
OldNameCall = hookmetamethod(game, "__namecall", function(...)
    local Args = {...}
    local Self = Args[1]
    if getnamecallmethod()=="FireServer" and tostring(Self)=="clientkick" or tostring(Self)=="PingReceive" or tostring(Self)=="reeeee" or tostring(Self)=="pong" or tostring(Self)=="Telebackup" or tostring(Self)=="ClientRequestTele" or tostring(Self) == "setChair" then
        return nil
    end
    if Args[1] == "Throw" then
    if Args[2] == "HumanoidRootPart" then 
       Args[3] = Vector3.new(1,2,1) 
    return
    end
    end 
    if getnamecallmethod()=="Kick" then
    return nil 
    end

    return OldNameCall(...)
end)



